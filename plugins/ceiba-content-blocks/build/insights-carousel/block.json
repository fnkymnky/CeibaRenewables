{
  "apiVersion": 3,
  "name": "ceiba/insights-carousel",
  "title": "Insights Carousel",
  "category": "ceiba-blocks",
  "icon": "slides",
  "description": "Manual selection of blog posts displayed in a carousel across all breakpoints.",
  "supports": {
    "anchor": true,
    "align": [
      "wide",
      "full"
    ],
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "attributes": {
    "includeIds": {
      "type": "array",
      "items": {
        "type": "number"
      },
      "default": []
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "script": "file:./view.js",
  "viewStyle": "file:./view.css",
  "render": "file:./render.php"
}