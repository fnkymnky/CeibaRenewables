{
  "apiVersion": 3,
  "name": "ceiba/image-frame",
  "title": "Image Frame",
  "category": "ceiba-blocks",
  "icon": "format-image",
  "supports": {
    "anchor": true,
    "align": [
      "wide",
      "full"
    ],
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "attributes": {
    "imageId": {
      "type": "number",
      "default": 0
    },
    "imageUrl": {
      "type": "string",
      "default": ""
    },
    "imageAlt": {
      "type": "string",
      "default": ""
    },
    "imageFit": {
      "type": "string",
      "default": "cover"
    },
    "focalPoint": {
      "type": "object",
      "default": {
        "x": 0.5,
        "y": 0.5
      }
    },
    "proportion": {
      "type": "string",
      "default": "wide"
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}