{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "slm/social-links",
  "title": "Social Links",
  "category": "widgets",
  "icon": "share",
  "description": "Display saved social links from Settings â†’ Social Links.",
  "textdomain": "social-links-manager",
  "supports": {
    "html": false,
    "align": ["left", "center", "right"]
  },
  "editorScript": "slm-block-social-links",
  "style": "file:../../assets/social-links.css",
  "editorStyle": "file:./editor.css",
  "attributes": {
    "brandColors": { "type": "boolean", "default": true },
    "showFacebook": { "type": "boolean", "default": true },
    "showTwitter": { "type": "boolean", "default": true },
    "showInstagram": { "type": "boolean", "default": true },
    "showLinkedin": { "type": "boolean", "default": true },
    "showWhatsapp": { "type": "boolean", "default": true },
    "showSnapchat": { "type": "boolean", "default": true },
    "showTiktok": { "type": "boolean", "default": true },
    "showYoutube": { "type": "boolean", "default": true }
  }
}
